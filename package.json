{
  "name": "simple-dict-translator",
  "version": "1.0.0",
  "description": "A simple translation module which accepts locale's with dictionary mappings",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/beije/simple-dict-translator.git"
  },
  "keywords": [
    "translation",
    "translator",
    "localization",
    "locale"
  ],
  "scripts": {
    "prepare": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "examplebuild": "node_modules/babel-cli/bin/babel.js example --out-dir lib/example",
    "example": "node lib/example/index.js",
    "test": "mocha --require babel-core/register ./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "Benjamin Horn (https://benjaminhorn.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0"
  },
  "dependencies": {}
}
